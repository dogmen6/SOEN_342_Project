class Deployed_Sensor {
	 private int sensorId;
	    private Location_Type location;
	    private Temperature_Type temperature;

	    public Deployed_Sensor(int sensorId, Location_Type location, Temperature_Type temperature) {
	        this.sensorId = sensorId;
	        this.location = location;
	        this.temperature = temperature;
	    }

	    public int getSensorId() {
	        return sensorId;
	    }

	    public Location_Type getLocation() {
	        return location;
	    }

	    public Temperature_Type getTemperature() {
	        return temperature;
	    }
}
